digraph G {
	node [color=lightblue2 style=filled]
	main -> account
	account -> accountcreated
	accountcreated -> account
	accountcreated -> gotoxy
	accountcreated -> login
	login -> gotoxy
	login -> loginsu
	loginsu -> account
	loginsu -> gotoxy
	loginsu -> login
	login -> display
	display -> transfermoney
	transfermoney -> gotoxy
	transfermoney -> display
	display -> checkbalance
	checkbalance -> gotoxy
	checkbalance -> display
	display -> account
	display -> logout
	logout -> gotoxy
	display -> gotoxy
	display -> login
	main -> gotoxy
	main -> login
}
